        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ctgu.ja.dao.ManagerDao">
    <select id="selectManager" parameterType="String" resultType="com.ctgu.ja.vo.Manager">
        select * from manager where managerId = #{managerId}
